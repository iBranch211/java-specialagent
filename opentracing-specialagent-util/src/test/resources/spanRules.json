{
  "*": [
    {
      "type": "log",
      "key": "db.statement",
      "output": [
        {
          "type": "log",
          "key": "*"
        }
      ]
    }
  ],
  "jedis": [
    {
      "type": "log",
      "key": "db.statement",
      "valueRegex": "(select).*",
      "output": [
        {
          "type": "log",
          "key": "jedis"
        }
      ]
    }
  ]
}
