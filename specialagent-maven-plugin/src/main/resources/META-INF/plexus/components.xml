<component-set xmlns="http://plexus.codehaus.org/xsd/components">
  <components>
    <component>
      <role>org.apache.maven.lifecycle.Lifecycle</role>
      <role-hint>fingerprint</role-hint>
      <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
      <configuration>
        <id>fingerprint</id>
        <default-phases>
          <generate-resources>io.opentracing.contrib:specialagent-maven-plugin:fingerprint</generate-resources>
        </default-phases>
      </configuration>
    </component>
    <component>
      <role>org.apache.maven.lifecycle.Lifecycle</role>
      <role-hint>fingerprint</role-hint>
      <implementation>org.apache.maven.lifecycle.Lifecycle</implementation>
      <configuration>
        <id>fingerprint</id>
        <phases>
          <phase>fingerprint</phase>
        </phases>
        <default-phases>
          <fingerprint>io.opentracing.contrib:specialagent-maven-plugin:fingerprint</fingerprint>
        </default-phases>
      </configuration>
    </component>
  </components>
</component-set>